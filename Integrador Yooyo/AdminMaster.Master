    <script>
        window.onload = function () {
            $('.overlaydark').hide();
            $('.dropdown-menu .dropdown-item').click(function (e) {
                e.stopPropagation();
            });
        };
        function listAmigos() {


            if ($('#listAmigos').is(":animated")) {
                return;//reject (no action)
            }
            console.log('entrou');
            if ($('#listAmigos').css('right') < "0") {
                $('.overlaydark').show();
                $('#listAmigos').animate({
                    right: '+=600px'

                });
            }
            else {
                $('.overlaydark').hide();
                $('#listAmigos').animate({
                    right: '-=600px'
                });
            }

        }
    </script>
